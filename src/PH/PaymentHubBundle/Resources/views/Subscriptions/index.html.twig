{% extends 'OroUIBundle:actions:index.html.twig' %}
{% import 'OroUIBundle::macros.html.twig' as UI %}

{% set gridName = 'ph-subscriptions-grid' %}
{% set pageTitle = 'Subscriptions'|trans %}


{% block navButtons %}
    {% if resource_granted('subscriptions.subscription_create') %}
        <div class="btn-group">
            {{ UI.addButton({
                'path': path('subscriptions.subscription_create'),
                'entity_label': 'Subscription'|trans
            }) }}
        </div>
    {% endif %}
{% endblock %}